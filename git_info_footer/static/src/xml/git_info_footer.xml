<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    
    <t t-name="git_info_footer.GitInfoFooter">
        <div t-att-class="'git-info-footer ' + statusClass">
            <span t-esc="displayText"/>
            <t t-if="state.error">
                <small class="git-info-error-details" t-esc="state.error"/>
            </t>
        </div>
    </t>
    
</templates>
